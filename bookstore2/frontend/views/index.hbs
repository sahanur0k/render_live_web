<script>
function updateNavbarForAuth() {
  // Desktop
  const loginBtn = document.getElementById('loginBtn');
  const registerBtn = document.getElementById('registerBtn');
  const youBtn = document.getElementById('youBtn');
  const logoutBtn = document.getElementById('logoutBtn');
  // Mobile
  const mobileLoginBtn = document.getElementById('mobileLoginBtn');
  const mobileRegisterBtn = document.getElementById('mobileRegisterBtn');
  const mobileYouBtn = document.getElementById('mobileYouBtn');
  const mobileLogoutBtn = document.getElementById('mobileLogoutBtn');

  checkLoginStatus().then(isLoggedIn => {
    if (isLoggedIn) {
      // Show You/Logout, hide Login/Register
      if (loginBtn) loginBtn.style.display = 'none';
      if (registerBtn) registerBtn.style.display = 'none';
      if (youBtn) youBtn.style.display = '';
      if (logoutBtn) logoutBtn.style.display = '';
      if (mobileLoginBtn) mobileLoginBtn.style.display = 'none';
      if (mobileRegisterBtn) mobileRegisterBtn.style.display = 'none';
      if (mobileYouBtn) mobileYouBtn.style.display = '';
      if (mobileLogoutBtn) mobileLogoutBtn.style.display = '';
    } else {
      // Show Login/Register, hide You/Logout
      if (loginBtn) loginBtn.style.display = '';
      if (registerBtn) registerBtn.style.display = '';
      if (youBtn) youBtn.style.display = 'none';
      if (logoutBtn) logoutBtn.style.display = 'none';
      if (mobileLoginBtn) mobileLoginBtn.style.display = '';
      if (mobileRegisterBtn) mobileRegisterBtn.style.display = '';
      if (mobileYouBtn) mobileYouBtn.style.display = 'none';
      if (mobileLogoutBtn) mobileLogoutBtn.style.display = 'none';
    }
  });
}

// Logout handler
function handleLogout(e) {
  e.preventDefault();
  localStorage.removeItem('token');
  document.cookie = 'token=; Max-Age=0; path=/;';
  window.location.reload();
}

document.addEventListener('DOMContentLoaded', function() {
  updateNavbarForAuth();
  // Attach logout handlers
  const logoutBtn = document.getElementById('logoutBtn');
  const mobileLogoutBtn = document.getElementById('mobileLogoutBtn');
  if (logoutBtn) logoutBtn.addEventListener('click', handleLogout);
  if (mobileLogoutBtn) mobileLogoutBtn.addEventListener('click', handleLogout);
});
</script>
