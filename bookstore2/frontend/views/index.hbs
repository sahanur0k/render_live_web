    max-width: 1200px;
    margin: 0 auto;
    padding: 0 20px;
  }
  
  .certifications-title {
    font-size: 2rem;
    text-align: center;
    margin-bottom: 15px;
    color: #0d47a1;
    position: relative;
  }
  
  .certifications-title::after {
    content: '';
    position: absolute;
    bottom: -10px;
    left: 50%;
    transform: translateX(-50%);
    width: 80px;
    height: 4px;
    background: linear-gradient(90deg, #1a73e8, #00acc1);
    border-radius: 2px;
  }
  
  .certifications-subtitle {
    text-align: center;
    color: #607d8b;
    margin-bottom: 50px;
    font-size: 1.1rem;
  }
  
  .certifications-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: 30px;
    max-width: 1000px;
    margin: 0 auto;
  }
  
  .certification-card {
    perspective: 1000px;
  }
  
  .certification-logo {
    height: 180px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    position: relative;
    transition: all 0.6s cubic-bezier(0.25, 0.8, 0.25, 1);
  }
  
  .logo-container {
    position: relative;
    width: 140px;
    height: 140px;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  
  .logo-circle {
    width: 120px;
    height: 120px;
    border-radius: 50%;
    background: white;
    display: flex;
    align-items: center;
    justify-content: center;
    box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);
    overflow: hidden;
    padding: 15px;
  }
  
  .official-logo {
    max-width: 100%;
    max-height: 100%;
    object-fit: contain;
    transition: all 0.4s ease;
    position: relative;
    z-index: 2;
  }
  
  .certification-card:hover .official-logo {
    transform: scale(1.05);
  }
  
  /* Animation Elements */
  .logo-shine, .logo-pulse, .logo-glow, .logo-sparkle {
    position: absolute;
    border-radius: 50%;
    z-index: 1;
  }
  
  /* ISO Shine Effect */
  .logo-shine {
    width: 120px;
    height: 120px;
    background: radial-gradient(circle at 30% 30%, rgba(255,255,255,0.8) 0%, rgba(255,255,255,0) 70%);
    opacity: 0;
    animation: cert-shine 3s infinite;
  }
  
  /* FSSAI Pulse Effect */
  .logo-pulse {
    width: 120px;
    height: 120px;
    border: 2px solid rgba(46, 125, 50, 0.3);
    animation: cert-pulse 2s infinite;
  }
  
  /* WHO Glow Effect */
  .logo-glow {
    width: 120px;
    height: 120px;
    box-shadow: 0 0 30px rgba(0, 147, 213, 0.4);
    opacity: 0;
    animation: cert-glow 2s infinite alternate;
  }
  
  /* GMP Sparkle Effect */
  .logo-sparkle {
    width: 120px;
    height: 120px;
    background: radial-gradient(circle, rgba(0, 137, 123, 0.2) 0%, rgba(0, 137, 123, 0) 70%);
    opacity: 0;
    animation: cert-sparkle 2s infinite;
  }
  
  .certification-tooltip {
    position: absolute;
    bottom: -60px;
    left: 50%;
    transform: translateX(-50%) scale(0.9);
    width: 100%;
    padding: 15px;
    background: white;
    border-radius: 8px;
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
    opacity: 0;
    transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    z-index: 10;
  }
  
  .certification-card:hover .certification-tooltip {
    opacity: 1;
    transform: translateX(-50%) scale(1);
    bottom: -70px;
  }
  
  .certification-tooltip h4 {
    font-size: 1rem;
    color: #0d47a1;
    margin-bottom: 5px;
    text-align: center;
  }
  
  .certification-tooltip p {
    font-size: 0.8rem;
    color: #607d8b;
    text-align: center;
  }
  
  /* Unique Animation Names to Prevent Conflicts */
  @keyframes cert-shine {
    0% { transform: translate(-30%, -30%); opacity: 0; }
    50% { opacity: 0.3; }
    100% { transform: translate(30%, 30%); opacity: 0; }
  }
  
  @keyframes cert-pulse {
    0% { transform: scale(1); opacity: 0.5; }
    70% { transform: scale(1.2); opacity: 0; }
    100% { transform: scale(1.2); opacity: 0; }
  }
  
  @keyframes cert-glow {
    from { opacity: 0; }
    to { opacity: 0.7; }
  }
  
  @keyframes cert-sparkle {
    0% { transform: scale(1); opacity: 0; }
    50% { opacity: 0.4; }
    100% { transform: scale(1.2); opacity: 0; }
  }
  
  /* Responsive */
  @media (max-width: 768px) {
    .certifications-grid {
      grid-template-columns: repeat(2, 1fr);
    }
  }
  
  @media (max-width: 480px) {
    .certifications-grid {
      grid-template-columns: 1fr;
    }
    
    .certification-logo {
      height: 150px;
    }
    
    .logo-container {
      width: 120px;
      height: 120px;
    }
    
    .logo-circle {
      width: 100px;
      height: 100px;
    }
    
    .logo-shine,
    .logo-pulse, 
    .logo-glow,
    .logo-sparkle {
      width: 100px;
      height: 100px;
    }
  }
