    }

    .hover-underline {
      position: relative;
      color: var(--primary-blue);
      text-decoration: none;
    }

    .hover-underline::after {
      content: '';
      position: absolute;
      bottom: -2px;
      left: 0;
      width: 0;
      height: 2px;
      background-color: var(--accent-teal);
      transition: var(--transition);
    }

    .hover-underline:hover::after {
